<h1>Follow Users</h1>
  <table class="table">
    <thead>
        <tr>
            <th>画像</th>
            <th>名前</th>
            <th colspan="3"></th>
        </tr>
    </thead>
      <tbody>
  <% @users.each do |user| %>
        <tr>
          <td><a><%= attachment_image_tag user, :profile_image, fallback: "no_image.jpg", size: "60x60" %></a></td>
          <td><%= user.name %></td>
        </tr>
  <% if current_user != user %>
  <% if user.followed_by?(current_user) %>
        <p><%=link_to "フォロー済み", user_relationships_path(user.id), method: :delete %></p>
        <% else %>
        <p><%=link_to "フォローする", user_relationships_path(user.id), method: :post %></p>
        <% end %>
        <% end %>
        <% end %>
      </tbody>
  </table>